<Page x:Class="AppointmentWPF2.Pages.Home"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AppointmentWPF2.Pages"
       xmlns:h="clr-namespace:AppointmentWPF2"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Home">

    <Grid Background="White" ButtonBase.Click="Grid_Click">
        <WrapPanel Margin="5,5,5,80">
            <h:NavBar Text="Home" ImageSource="/Images/Home.png" NavUri="/Pages/Home.xaml" IsManipulationEnabled="True"/>
            <h:NavBar Text="Customers" ImageSource="/Images/Customer.jfif" NavUri="/Pages/Customers.xaml"/>
            <h:NavBar Text="Brokers" ImageSource="/Images/broker1.png" NavUri="/Pages/Brokers.xaml"/>
            <h:NavBar Text="Appointments" ImageSource="/Images/Calendar.png" NavUri="/Pages/Appointment.xaml"/>
            <h:NavBar Text="Login" ImageSource="/Images/Login4.png" NavUri="/Pages/Login.xaml"/>
            <h:NavBar Text="Logout" ImageSource="/Images/Logout4.png" NavUri="/Pages/Logout.xaml"/>
            <h:NavBar Text="Register" ImageSource="/Images/register.png" NavUri="/Pages/Register.xaml"/>
        </WrapPanel>
    </Grid>
</Page>
